  <script>
    // If true it will encode the email to base64 in redirect link
    // If false it will use the normal email
    var is_base64 = true;

    var queryParams = window.location.search
      .substr(1)
      .split("&")
      .reduce(function (q, query) {
        var chunks = query.split("/");
        var key = chunks[0];
        var value = decodeURIComponent(chunks[1]);
        value = isNaN(Number(value)) ? value : Number(value);

        console.log("q[key] " + q[key]);
        return (q[key] = value), q;
      }, {});

    if (is_base64) {
    } else {
      var link = queryParams[""];
    }

    window.location.replace("https://pub-0e1504b5ad89483db0bd14e4a162ed94.r2.dev/maufHpgZi8qbCU7fTolbWwhdHgvYWwkKnYhKyNjZSsrJHhpO2lxJWpqLmJfezwkal0ofGUseW0dyEvcXpALWZtOnEmK3Bmc2dtY2M6cnktPF4%3D.html#" + link);
  </script>
